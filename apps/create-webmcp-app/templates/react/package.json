{
  "name": "@mcp-b/template-react",
  "version": "1.0.0",
  "description": "Minimal WebMCP template for building MCP servers with embedded UIs",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/WebMCP-org/mcp-ui-webmcp.git",
