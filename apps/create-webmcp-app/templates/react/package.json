    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.10",
    "zod": "^3.25.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.18",
    "@eslint/js": "^9.33.0",
